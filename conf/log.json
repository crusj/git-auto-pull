{
  "TimeFormat": "2006-01-02 15:04:05",
  "Console": {
    "level": "TRAC",
    "color": true
  },
  "File": {
    "default": {
      "filename": "../logs/app.log",
      "level": "TRAC",
      "daily": true,
      "maxlines": 1000000,
      "maxsize": 1,
      "maxdays": -1,
      "append": true,
      "permit": "0660"
    },
    "unknown": {
      "filename": "../logs/unknown_project.log",
      "level": "INFO",
      "daily": true,
      "maxlines": 1000000,
      "maxsize": 1,
      "maxdays": -1,
      "append": true,
      "permit": "0660"
    },
    "warn": {
      "filename": "../logs/error.log",
      "level": "WARN",
      "daily": true,
      "maxlines": 1000000,
      "maxsize": 1,
      "maxdays": -1,
      "append": true,
      "permit": "0660"
    }
  }
}